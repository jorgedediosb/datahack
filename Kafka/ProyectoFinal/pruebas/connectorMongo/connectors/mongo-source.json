{
    "name": "mongo-source",
    "config": {
      "connector.class": "com.mongodb.kafka.connect.MongoSourceConnector",
      "tasks.max": "1",
      "connection.uri": "mongodb://admin:admin@mongodb:27017",
      "database": "sentiment_analysis",
      "collection": "results",
      "topic.prefix": "mongo-",
      "poll.max.batch.size": "1000",
      "poll.await.time.ms": "5000",
      "batch.size": "0"
    }
  }
  